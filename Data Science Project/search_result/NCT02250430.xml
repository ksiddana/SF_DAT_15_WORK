  <primary_outcome>
    <measure>Intensity and duration of erythema following application of SB204 2% and SB204 4%.</measure>
    <time_frame>3 days</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <secondary_outcome>
    <measure>pH on the surface of the skin after topical application of 2 concentrations of SB204</measure>
    <time_frame>3 days</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Actual">8</enrollment>
  <condition>Acne Vulgaris</condition>
